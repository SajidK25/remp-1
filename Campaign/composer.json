{
    "name": "laravel/laravel",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "type": "project",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "repositories": [
        {
            "type": "path",
            "url": "../Composer/*",
            "options": {
                "symlink": false
            }
        }
    ],
    "require": {
        "php": "^7.4",
        "ext-json": "*",
        "cache/predis-adapter": "^1.0",
        "deployer/deployer": "^6.0",
        "deployer/recipes": "^6.0",
        "doctrine/dbal": "^2.13",
        "fico7489/laravel-pivot": "^3.0",
        "fideloper/proxy": "^4.4",
        "fruitcake/laravel-cors": "^2.0",
        "fzaninotto/faker": "~1.4",
        "geoip2/geoip2": "~2.0",
        "guzzlehttp/guzzle": "^6.3.1|^7.0.1",
        "kouz/laravel-airbrake": "^0.7.0",
        "laracasts/flash": "^3.2",
        "laravel/framework": "^7.29",
        "laravelcollective/html": "^6.2",
        "league/uri": "^5.0",
        "nicolaslopezj/searchable": "^1.13",
        "php-amqplib/php-amqplib": "^2.7",
        "piwik/device-detector": "^3.10",
        "predis/predis": "^1.1",
        "recca0120/laravel-tracy": "^1.8",
        "remp/laravel-helpers": "*",
        "remp/laravel-sso": "*",
        "remp/laravel-widgets": "*",
        "remp/multi-armed-bandit": "*",
        "sentry/sentry-laravel": "^2.5",
        "spatie/laravel-searchable": "^1.9",
        "symfony/psr-http-message-bridge": "^1.0",
        "tightenco/ziggy": "^1.2",
        "ukfast/laravel-health-check": "^1.5",
        "yajra/laravel-datatables-oracle": "^9.18"
    },
    "require-dev": {
        "barryvdh/laravel-ide-helper": "^2.8",
        "friendsofphp/php-cs-fixer": "^3.0",
        "josiasmontag/laravel-redis-mock": "~1.2",
        "mockery/mockery": "^1.3.1",
        "nunomaduro/collision": "^4.3",
        "phpunit/phpunit": "^8.5.8|^9.3.3",
        "laravel/tinker": "^2.0",
        "fakerphp/faker": "^1.9.1",
        "facade/ignition": "^2.0"
    },
    "autoload": {
        "classmap": [
            "database"
        ],
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-root-package-install": [
            "php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true
    }
}
