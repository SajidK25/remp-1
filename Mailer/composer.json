{
    "name": "nette/web-project",
    "description": "Nette: Standard Web Project",
    "type": "project",
    "license": ["MIT"],
    "config": {
        "sort-packages": true
    },
    "repositories": [
        {
            "type": "path",
            "url": "../Composer/*",
            "options": {
                "symlink": false
            }
        }
    ],
    "require": {
        "php": "^7.3",
        "ext-intl": "*",
        "ext-json": "*",
        "deployer/deployer": "^6.0",
        "deployer/recipes": "^6.0",
        "embed/embed": "^3.4",
        "guzzlehttp/guzzle": "^6.5",
        "halaxa/json-machine": "^0.3",
        "justinrainbow/json-schema": "^5.2",
        "latte/latte": "^2.5",
        "league/event": "^3.0",
        "mailgun/mailgun-php": "^3.1",
        "monolog/monolog": "^2.1",
        "nette/application": "^3.0",
        "nette/bootstrap": "^3.0",
        "nette/caching": "^3.0",
        "nette/database": "^3.0",
        "nette/di": "^3.0",
        "nette/finder": "^2.5",
        "nette/forms": "^3.0",
        "nette/http": "^3.0",
        "nette/mail": "^3.0",
        "nette/robot-loader": "^3.0",
        "nette/security": "^3.0",
        "nette/utils": "^3.0",
        "oops/webpack-nette-adapter": "^1.0",
        "php-amqplib/php-amqplib": "^2.7",
        "predis/predis": "^1.1",
        "remp/journal": "@dev",
        "remp/multi-armed-bandit": "@dev",
        "remp/nette-sso": "@dev",
        "robmorgan/phinx": "^0.12",
        "rootpd/nette-sentry": "^1.1",
        "symfony/console": "^5.2",
        "symfony/http-client": "^5.1",
        "tomaj/hermes": "^4.0",
        "tomaj/nette-api": "^2.1",
        "tracy/tracy": "^2.6",
        "twig/twig": "^3.1",
        "vlucas/phpdotenv": "^5.2"
    },
    "require-dev": {
        "roave/security-advisories": "dev-master",
        "phpunit/phpunit": "^9.4",
        "symfony/var-dumper": "^5.2",
        "phpstan/phpstan": "^0.12",
        "squizlabs/php_codesniffer": "^3.2",
        "friendsofphp/php-cs-fixer": "^2.11",
        "phpstan/phpstan-nette": "^0.12.10"
    },
    "autoload": {
        "psr-4": {
            "Remp\\MailerModule\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    }
}
